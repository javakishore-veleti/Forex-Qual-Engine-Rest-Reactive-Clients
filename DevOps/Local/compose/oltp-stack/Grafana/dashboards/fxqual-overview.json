{
  "id": null,
  "uid": "fxqual-overview",
  "title": "FX Qual Engine â€“ Overview",
  "tags": ["fxqual", "sre", "otel"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "stat",
      "title": "Total FX Qual Requests (RPS)",
      "datasource": "Prometheus",
      "gridPos": { "x": 0, "y": 0, "w": 8, "h": 5 },
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{service=\"fx-qual-engine\"}[1m]))",
          "format": "time_series"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Error Rate %",
      "datasource": "Prometheus",
      "gridPos": { "x": 8, "y": 0, "w": 8, "h": 5 },
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{service=\"fx-qual-engine\",status!~\"2..\"}[1m])) / sum(rate(http_server_requests_seconds_count{service=\"fx-qual-engine\"}[1m])) * 100"
        }
      ]
    },
    {
      "type": "graph",
      "title": "FX Qual Request Latency (p95)",
      "datasource": "Prometheus",
      "gridPos": { "x": 0, "y": 5, "w": 16, "h": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket{service=\"fx-qual-engine\"}[5m])) by (le))"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Qualified Quotes per Minute",
      "datasource": "Prometheus",
      "gridPos": { "x": 0, "y": 13, "w": 16, "h": 8 },
      "targets": [
        { "expr": "sum(rate(fxqual_qualified_quotes_total[1m]))" }
      ]
    }
  ]
}
