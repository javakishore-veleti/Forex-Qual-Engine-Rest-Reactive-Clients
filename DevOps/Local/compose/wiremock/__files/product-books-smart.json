{
  "bookDetails": [
    {{#each (jsonPath request.body '$.bookIds')}}
    {
      "bookId": "{{this}}",
      "riskCategory": "{{#ifeq this 'BOOK-HIGH'}}HIGH{{else}}LOW{{/ifeq}}",
      "limits": {
        "min": {{#ifeq this 'BOOK-HIGH'}}1000000{{else}}10000{{/ifeq}},
    "max": {{#ifeq this 'BOOK-HIGH'}}50000000{{else}}5000000{{/ifeq}}
  }
  }{{#unless @last}},{{/unless}}
    {{/each}}
  ]
}
