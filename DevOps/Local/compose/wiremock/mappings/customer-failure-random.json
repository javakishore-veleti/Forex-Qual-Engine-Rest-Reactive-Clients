{
  "request": {
    "method": "GET",
    "urlPattern": "/api/customer/.*"
  },
  "response": {
    "status": 200,
    "bodyFileName": "customer-smart-response.json",
    "transformers": ["response-template"],
    "delayDistribution": {
      "type": "uniform",
      "lower": 0,
      "upper": 2000
    }
  }
}
