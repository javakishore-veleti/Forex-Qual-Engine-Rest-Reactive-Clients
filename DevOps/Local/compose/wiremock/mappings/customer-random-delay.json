{
  "priority": 20,
  "request": {
    "urlPattern": "/api/customer/.*",
    "method": "GET"
  },
  "response": {
    "status": 200,
    "bodyFileName": "customer-smart-response.json",
    "delayDistribution": {
      "type": "uniform",
      "lower": 50,
      "upper": 500
    },
    "transformers": ["response-template"],
    "headers": {
      "Content-Type": "application/json"
    }
  }
}
